{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "src/types/**.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "back_types": [
        "./.generated/rest.ts"
      ]
    },
    "noImplicitAny": false,
    "preserveValueImports": false,
    "strictNullChecks": false,
    "isolatedModules": false,
    "importsNotUsedAsValues": "preserve",
    "allowJs": true,
    "types": [
      "node"
    ]
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
